{
  "name": "developersnl/bullhorn-client-bundle",
  "type": "symfony-bundle",
  "description": "Provides a simple client for the Bullhorn REST API.",
  "keywords": ["bullhorn", "client", "symfony", "bundle", "developersnl"],
  "license": "MIT",
  "authors": [
    {
      "name": "Lex de Willigen",
      "email": "l.dewilligen@developers.nl"
    },
    {
      "name": "Kaj de Munter",
      "email": "k.demunter@developers.nl"
    }
  ],
  "require": {
    "symfony/framework-bundle": "~2.8|~3.0|~4.0|~5.0",
    "league/oauth2-client": "^2.5",
    "guzzlehttp/guzzle": "^7.2",
    "snc/redis-bundle": "^3.0",
    "psr/simple-cache": "^1.0",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.4"
  },
  "autoload": {
    "psr-4": { "Developersnl\\BullhornClientBundle\\": "src" }
  },
  "autoload-dev": {
    "psr-4": {"Developersnl\\BullhornClientBundle\\Tests\\": "tests"}
  }
}